<!--FORMULARIO Iniciar seion-->



<div class="card m-5">
    <h5 class="card-header">INICIAR SESION </h5>
    <div class="card-body">
        <form action="/login" method="POST">
            <div class="form-group">
                <label for="correo" class="form-label p-1">Número de celular o correo electronico:</label>
                <input type="text" class="form-control" id="correo" name="correo">

                <label for="contraseña" class="form-label p-1">contraseña:</label>
                <input type="password" class="form-control" id="contraseña" name="contraseña">

            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-outline-dark p-1 mt-5">INGRESAR</button>
                <p>¿Has olvidado tu contraseña?</p>
            </div>

            <div class="form-group">
                <button type="submit"  class="btn btn-outline-dark p-1 mt-5"><i class="bi bi-google"></i>Iniciar con Google</button>
            
                <button type="submit"  class="btn btn-outline-dark p-1 mt-5"><i class="bi bi-facebook"></i>Iniciar con Facebook</button>
            </div>


        </form>
    </div>
  </div>






